{
  "name": "emartech/php-webapp",
  "description": "core function to create web applications",
  "minimum-stability": "stable",
  "license": [
    "MIT"
  ],
  "prefer-stable" : true,
  "require": {
    "php": "~7.3",
    "psr/log": "1.1.4",
    "psr/http-message": "1.0.1",
    "emartech/escher" : "1.2.1",
    "emartech/php-http-status": "0.0.1",
    "guzzlehttp/guzzle": "6.5.5",
    "monolog/monolog": "2.2.0",
    "nikic/fast-route": "v1.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.15",
    "phpunit/php-invoker": "3.1.1",
    "emartech/php-test-helpers": "1.1.1",
    "fqqdk/packagist-publisher": "1.2.6"
  },
  "autoload": {
    "psr-4": {
      "Emartech\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/"
    }
  }
}
